{
	"schemaVersion": 1,
	"id": "agribot",
	"version": "${version}",
	"name": "Agri Bot",
	"description": "Bot de farming automatique pour Minecraft",
	"authors": [
		"Nix"
	],
	"contact": {
		"sources": "https://github.com/Dayti-0/Agri-Bot-1.21.4"
	},
	"license": "CC0-1.0",
	"icon": "assets/agribot/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "fr.nix.agribot.AgriBot",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "fr.nix.agribot.AgriBotClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"agribot.mixins.json",
		{
			"config": "agribot.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}